{
  "name": "hh-hw-infrastructure",
  "version": "0.0.0",
  "description": "",
  "author": "Andrey Voronin <AVor0n@yandex.ru>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier . --write --ignore-unkown",
    "typecheck": "tsc --noEmit",
    "lint:style": "stylelint ./src/**/*.{css,scss}",
    "fix:style": "yarn lint:style --fix",
    "lint:eslint": "eslint ./src/ --ext=.ts,.tsx",
    "fix:eslint": "yarn lint:eslint --fix",
    "lint": "concurrently 'yarn:typecheck' 'yarn:lint:*' --names ts --kill-others-on-fail --prefix-colors yellow,magenta,blue",
    "fix": "concurrently 'yarn:fix:*' --prefix-colors magenta,blue",
    "preview": "vite preview",
    "deploy": "yarn build && gh-pages -d dist",
    "postinstall": "husky install"
  },
  "dependencies": {
    "postcss-scss": "4.0.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stylelint-scss": "5.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/eslint-parser": "7.23.3",
    "@hh.ru/eslint-config": "15.1.1",
    "@hh.ru/eslint-plugin-import-rules": "1.5.2",
    "@hh.ru/stylelint-config-hh": "10.2.1",
    "@types/babel__core": "7.20.5",
    "@types/node": "20.10.3",
    "@types/react": "18.2.41",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react-swc": "3.5.0",
    "concurrently": "8.2.2",
    "eslint": "8.55.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-dollar-sign": "1.0.2",
    "eslint-plugin-etc": "2.0.3",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "gh-pages": "6.1.0",
    "husky": "8.0.3",
    "less": "4.2.0",
    "nano-staged": "0.8.0",
    "postcss": "8.4.32",
    "prettier": "3.1.0",
    "sass": "1.69.5",
    "stylelint": "15.11.0",
    "stylelint-config-recommended": "13.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.7.0",
    "stylelint-order": "6.0.3",
    "stylelint-semantic-groups": "1.2.0",
    "stylelint-use-logical-spec": "5.0.0",
    "typescript": "4.9.5",
    "vite": "5.0.4"
  },
  "packageManager": "yarn@4.0.2"
}
